{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Bracket",
	"patterns": [
		{
			"include": "#keywords"
		},{
			"include": "#comments"
		},{
			"include": "#strings"
		},{
			"include": "#variable"
		},{
			"include": "#assign"
		},{
			"include": "#this"
		},{
			"include": "#end"
		}
	],
	"repository": {
		"variable": {
			"name": "variable.bcrt",
			"begin": "'",
			"end": "'",
			"patterns": [
			]
		},
		"comments": {
			"name": "comment.bcrt",
			"begin": "\\\\",
			"end": "\\\\",
			"patterns": [
			]
		},
		"assign": {
			"name": "equals.bcrt",
			"patterns": [
				{
					"name": "assign.char.bcrt",
					"match": "@"
				}
			]
		},
		"this": {
			"name": "self.bcrt",
			"patterns": [
				{
					"name": "this.char.bcrt",
					"match": ":"
				}
			]
		},
		"end": {
			"name": "end.bcrt",
			"patterns": [
				{
					"name": "end.char.bcrt",
					"match": ";"
				}
			]
		}
	},
	"scopeName": "text.bcrt"
}