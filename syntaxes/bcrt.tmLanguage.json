{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Bracket",
	"patterns": [
		{
			"include": "#comments"
		},{
			"include": "#variable"
		},{
			"include": "#value"
		},{
			"include": "#assign"
		},{
			"include": "#end"
		},{
			"include": "#index"
		}
	],
	"repository": {
		"variable": {
			"name": "variable.name.bcrt",
			"begin": "'",
			"end": "'",
			"patterns": [
			]
		},
		"value": {
			"name": "constant.other.value.bcrt",
			"begin": "{",
			"end": "}",
			"patterns": [
				{
					"include": "#value"
				},{
					"include": "#variable"
				},{
					"include": "#assign"
				},{
					"include": "#this"
				},{
					"include": "#number"
				},{
					"include": "#index"
				}
			]
		},
		"index": {
			"name": "constant.other.index.bcrt",
			"begin": "\\[",
			"end": "\\]",
			"patterns": [
				{
					"include": "#value"
				},{
					"include": "#variable"
				},{
					"include": "#this"
				},{
					"include": "#number"
				}
			]
		},
		"number": {
			"name": "constant.numeric.bcrt",
			"patterns": [],
			"match": "\\d+"
		},
		"comments": {
			"name": "comment.block.bcrt",
			"begin": "\\\\",
			"end": "\\\\",
			"patterns": []
		},
		"assign": {
			"name": "storage.modifier.bcrt",
			"patterns": [],
			"match": "@"
		},
		"this": {
			"name": "variable.name.bcrt",
			"patterns": [],
			"match": ":"
		},
		"end": {
			"name": "punctuation.terminator.bcrt",
			"patterns": [],
			"match": ";"
		}
	},
	"scopeName": "text.bcrt"
}