{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Bracket",
	"patterns": [
		{
			"include": "#comments"
		},{
			"include": "#variable"
		},{
			"include": "#value"
		},{
			"include": "#assign"
		},{
			"include": "#end"
		}
	],
	"repository": {
		"variable": {
			"name": "variable.bcrt",
			"begin": "'",
			"end": "'",
			"patterns": [
			]
		},
		"value": {
			"name": "value.bcrt",
			"begin": "{",
			"end": "}",
			"patterns": [
				{
					"include": "#value"
				},{
					"include": "#variable"
				},{
					"include": "#assign"
				},{
					"include": "#this"
				},{
					"include": "#number"
				}
			]
		},
		"number": {
			"name": "number.bcrt",
			"patterns": [],
			"match": "\\d+"
		},
		"comments": {
			"name": "comment.bcrt",
			"begin": "\\\\",
			"end": "\\\\",
			"patterns": []
		},
		"assign": {
			"name": "assign.bcrt",
			"patterns": [],
			"match": "@"
		},
		"this": {
			"name": "self.bcrt",
			"patterns": [],
			"match": ":"
		},
		"end": {
			"name": "end.bcrt",
			"patterns": [],
			"match": ";"
		}
	},
	"scopeName": "text.bcrt"
}